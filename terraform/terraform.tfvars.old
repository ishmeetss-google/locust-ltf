# terraform.tfvars (Root - Example - Public Endpoint)

# --- Global Variables (from root variables.tf) ---
project_id     = "vertex-platform" # Replace with your actual project ID
project_number = 158892293228      # Replace with your actual Google cloud project number.
region         = "us-central1"     # Replace with your desired region

# --- Vertex AI Vector Search Module Variables (from modules/vertex-ai-vector-search/variables.tf) ---

# -- Cloud Storage Bucket (Existing) --
existing_bucket_name = "None" # Replace with your actual bucket name
embedding_data_path  = "None" # Replace with your embedding folder path

# -- Index Settings --
index_dimensions                  = 768 # default vablues
index_approximate_neighbors_count = 150 # default vablues

# -- Endpoint Settings --
endpoint_public_endpoint_enabled = true # Enable public endpoint

# -- Deployed Index Settings --
deployed_index_resource_type = "automatic" # Or "dedicated"

# -- GKE Autopilot Settings --
image = "us-central1-docker.pkg.dev/vertex-platform/locust-docker-repo/locust-image@sha256:f6c9dcac370f1a47b576fab1b05860b7011a0322e52824bdeadecf4888010811"


vector_search_index_id = "projects/vertex-platform/locations/us-central1/indexes/1688423249752358912"
